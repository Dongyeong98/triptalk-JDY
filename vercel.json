{
  "rewrites": [
    {
      "source": "/address/api/plans/:plannerId/details",
      "destination": "https://triptalk.xyz/api/plans/:plannerId/details"
    },
    {
      "source": "/address/api/main",
      "destination": "https://triptalk.xyz/api/main"
    },
    {
      "source": "/address/api/users/profile",
      "destination": "https://triptalk.xyz/api/users/profile"
    },
    {
      "source": "/address/api/users/update/profile",
      "destination": "https://triptalk.xyz/api/users/update/profile"
    },
    {
      "source": "/address/api/plans",
      "destination": "https://triptalk.xyz/api/plans"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}